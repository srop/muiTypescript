{
  "compilerOptions": {
    "baseUrl": "src",
    // "paths": {
    //   "@Components/*": [
    //     "components/*"
    //   ],
    //   "@Context/*": [
    //     "context/*"
    //   ]
    // },
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext",
      "es2015",
      "es2016",
      "es2017",
      "es2019",
      "es2020"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "noImplicitAny": false,
    "suppressImplicitAnyIndexErrors": true,
    "outDir": "target/classes/static/app",
    "checkJs": false,
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules"
  ]
}